{
    "env": {
        "browser": true,
        "jasmine": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "eol-last": ["error", "always"],
        "indent": ["error", 4, {
            "ObjectExpression": "first",
            "SwitchCase": 1
        }],
        "no-multiple-empty-lines": ["error", {
            "max": 2,
            "maxEOF": 1
        }],
        "no-trailing-spaces": "error",
        "semi": ["error", "always"],
        "space-before-blocks": "error"
    }
}
